1. a=10;
  b=++a;
  System.out.println(b)
2. a=10;
  b=a++;
  System.out.println(a);
3. int a=20;
  a= ++a + 1;
  System.out.println(a);
4. int a=20;
  a= a++ + --a;
  System.out.println(a);
