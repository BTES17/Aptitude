
class Student implements Comparable<student>{
int marks;
int rollno;
String name;
Student (int marks, int rollno, String name){
	this.marks=marks;
	this.rollno=rollno;
	this.name= name;
}
	@Override
	public int compareTo(student o) {
		if (this.marks>o.marks)
		return 1;
		else if (this.marks==o.marks)
			return 0;
		else 
			return -1;
	}
	
}


import java.util.ArrayList;
import java.util.Collection;
import java.util.Comparator;
import java.util.List;
import java.util.Scanner;

public class MyCollections {

	public static void main(String[] args) {
		student stu1= new student(2,5,"abc");
		student stu2= new student(1,5,"abc1");
		student stu3= new student(0,5,"abc2");
		ArrayList<student>  list= new ArrayList<student>();
	
		list.add(stu1);
		list.add(stu2);
		list.add(stu3);
		java.util.Collections.sort(list);
		for(student e: list)
		{
			System.out.println(e.marks);
		}
	list.clear();
		list.add(stu1);
		list.add(stu2);
		list.add(stu3);
		java.util.Collections.sort(list,new marks());
		for(student e: list)
		{
			System.out.println(e.marks);
		}
		list.clear();
		list.add(stu1);
		list.add(stu2);
		list.add(stu3);
		java.util.Collections.sort(list,new name());
		for(student e: list)
		{
			System.out.println(e.name);
		}
	}

}
